
@{
    Layout = null;
    ViewBag.Title = "Page Not Found";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@ViewBag.Title | AOP! </title>

    <!-- Bootstrap core CSS -->
    <!-- Global stylesheets -->
    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="~/Content/core.css" rel="stylesheet" type="text/css">
    <link href="~/Content/components.css" rel="stylesheet" type="text/css">
    <link href="~/Content/colors.css" rel="stylesheet" type="text/css">
    <!-- /global stylesheets -->

</head>

<body class="login-container">

    <!-- Page container -->
    <div class="page-container">

        <!-- Page content -->
        <div class="page-content">

            <!-- Main content -->
            <div class="content-wrapper">

                <!-- Content area -->
                <div class="content pb-20">

                    <div class="panel panel-body">
                        <div class="text-center">
                            <div class="icon-object border-slate-300 text-slate-300"><img src="~/images/rdlp.png" height="70px" width="80px" /></div>
                            <div class="alert bg-warning">
                                <h1>404</h1>
                                <h3><span class="text-bold">Ooops!</span> The page you have requested cannot be found.</h3>
                            </div>
                        </div>

                    </div>

                </div>
                <!-- /content area -->

            </div>
            <!-- /main content -->

        </div>
        <!-- /page content -->

    </div>
    <!-- /page container -->

</body>
</html>
